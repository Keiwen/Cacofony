
<script type="text/javascript">

    function cacoawCall_{{ id }}() {
        $.ajax({
            type: '{{ method }}',
            url: '{{ url }}',
            data: '{{ parameters }}',
            dataType : 'html',
            success: function(data, textStatus, jqXHR) {
                $('#cacoawContainer_{{ id }}').parent().html(data);
            },
            error: function(jqXHR,textStatus, thrownError) {
                $('#cacoawLoader_{{ id }}').addClass('hidden');
                $('#cacoawError_{{ id }}').removeClass('hidden');
            }
        });
    }

</script>
