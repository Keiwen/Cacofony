
<script type="text/javascript">

    $.ajax({
        type: '{{ method }}',
        url: '{{ url }}',
        data: '{{ parameters }}',
        dataType : 'html',
        success: function(data, textStatus, jqXHR) {
            $('#cawContainer_{{ id }}').parent().html(data);
        },
        error: function(jqXHR,textStatus, thrownError) {
            $('#cawLoader_{{ id }}').addClass('hidden');
            $('#cawError_{{ id }}').removeClass('hidden');
        }
    });


</script>
