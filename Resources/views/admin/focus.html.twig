{% extends '@EasyAdmin/default/list.html.twig' %}

{% block content_title %}
    {% apply spaceless %}
    {% set _default_title = 'list.page_title'|trans(_trans_parameters, 'EasyAdminBundle') %}
    {{ (_entity_config.list.title is defined ? _entity_config.list.title|trans(_trans_parameters) : _default_title)|raw }}
    <br/>
    ({{ _request_parameters.focusFromEntity }} {{ _request_parameters.focusFromName }})
    {% endapply %}
{% endblock %}

{% block search_form %}
    {{ parent() }}
    <input type="hidden" name="focusFromEntity" value="{{ _request_parameters.focusFromEntity }}">
    <input type="hidden" name="focusFromId" value="{{ _request_parameters.focusFromId }}">
    <input type="hidden" name="focusFromName" value="{{ _request_parameters.focusFromName }}">
{% endblock %}


{% block filters_action %}
    {# remove filters action, cannot override for focus #}
{% endblock filters_action %}
